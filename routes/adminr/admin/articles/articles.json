        "name": "articles.index",
        "controller": "App\\Http\\Controllers\\Admin\\ArticleController",
        "action": "index",
        "method": "get"
    },
    "create": {
        "route": "articles/create",
        "middleware": ["web", "permission:articles_create"],
        "name": "articles.create",
        "controller": "App\\Http\\Controllers\\Admin\\ArticleController",
